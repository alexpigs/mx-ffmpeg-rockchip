    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/ffmpeg_g",
            "args": [
                "-loglevel",
                "warning",
                //"-hwaccel","rkmpp",
                //"-hwaccel_output_format","drm_prime",
                "-stream_loop",
                "-1",
                "-i",
                // "rtmp://47.100.44.74/myapp/9b57184ca3ac3cab71cbc151e619a764",
                "/home/zxe/test2.mp4",
                //"-vf","hwupload,vpp_rkrga=transpose=cclock,hwdownload,format=nv12",
                "-vf",
                "scale=1080:-1,format=nv12",
                "-vsync",
                "2",
                "-f",
                "mxcam",
                "-phone",
                "1",
                "-listen_port",
                "10201",
                "-listen_ip",
                "0.0.0.0",
                "-"
            ],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": []
        }
    ]
}